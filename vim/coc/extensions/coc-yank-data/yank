f6a5cc82702b88a6c21d5b93394e7cee|file:///Users/rfarrer/Wayfair/homebase/app/src/admin-demos/progress-bar.js|5|1|V|javascript
	const Demo = () => {
	  const [progress, setProgress] = useState(0);
	  const timer = useRef(null);
	
	  const handleStart = () => {
	    let update = progress;
	
	    if (!timer.current) {
	      timer.current = setInterval(() => {
	        if (update >= 100) {
	          clearInterval(timer.current);
	          return;
	        }
	
	        update += 10;
	
	        setProgress(update);
	      }, 500);
	    }
	  };
	
	  const handleReset = () => {
	    clearInterval(timer.current);
	    timer.current = null;
	    setProgress(0);
	  };
	
	  React.useEffect(() => {
	    return () => {
	      if (timer.current) {
	        clearInterval(timer.current);
	      }
	    };
	  }, []);
	
	  return (
	    <Block>
	      <Button onClick={handleStart}>Start</Button>
	      <Button onClick={handleReset}>Reset</Button>
	      <Block mt="medium">
	        <ProgressBar
	          showInnerLabel={false}
	          showOuterLabel
	          current={progress}
	          min={0}
	          total={100}
	          status="error"
	        />
	      </Block>
	    </Block>
	  );
	};
