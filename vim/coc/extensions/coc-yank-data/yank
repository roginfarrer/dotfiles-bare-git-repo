1825ab8f0d51ca32f8a657e3be2d4dc2|file:///Users/rfarrer/Wayfair/homebase/packages/admin-components/src/selectable/selectable.scss|19|1|V|scss
	
	  &.is-disabled {
	    opacity: 0.5;
	    cursor: not-allowed;
	  }
	
	  &:not(.is-disabled) {
	    &:hover,
	    &.has-focus {
	      #{$SelectableUI} {
	        box-shadow: $box-shadow;
	      }
	
	      &.is-checked #{$SelectableUI} {
	        // One box-shadow creates the regular shadow beneath the element,
	        // while the other thickens the border without shifting elements around it
	        // Works similar to outline, expect it accommodates
	        // the curve of border-radius
	        box-shadow: $box-shadow, $border-effect--active;
	      }
	    }
	
	    // Flatten the box-shadow when being clicked
	    &.is-checked,
	    &:hover:active,
	    &.has-focus:active,
	    &:active {
	      #{$SelectableUI} {
	        border-color: $border-color--primary;
	        box-shadow: $border-effect--active;
	      }
	    }
	
	    // Don't flatten the box-shadow on *checked* radio buttons
	    // since clicking a radio button doesn't uncheck it
	    // so it can be misleading
	    &#{$root}--radio.is-checked:active #{$SelectableUI} {
	      box-shadow: $box-shadow, $border-effect--active;
	    }
	  }
	
	  #{$SelectableUI} {
	    display: inline-block;
	    border: 1px solid $border-color--base;
	    transition: box-shadow 0.1s ease, border-color 0.1s ease;
	    &--circle {
	      border-radius: 50%;
	      overflow: hidden;
	    }
	  }
	}
