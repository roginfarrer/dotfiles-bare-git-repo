	////
	/// Tabbed Content
	///
	/// @author    Frontend Design Systems <FrontendDesignSystems@wayfair.com>
	/// @copyright 2019 Wayfair LLC - All rights reserved
	////
	import {css} from '@emotion/core';
	import styled from '@emotion/styled';
	import {lighten} from 'polished';
	
	export const Root = styled.div`
	  position: relative;
	  margin-bottom: ${props => props.theme.spacing.large};
	  display: ${({isVertical}) => isVertical && 'flex'};
	`;
	
	export const List = styled.ul`
	  display: flex;
	  ${props =>
	    props.isVertical
	      ? css`
	          border-right: 1px solid ${props.theme.color.blackTint3};
	          flex-direction: column;
	          margin-right: 20px;
	          li + li {
	            margin-top: 5px;
	          }
	        `
	      : css`
	          border-bottom: 1px solid ${props.theme.color.blackTint3};
	          margin-bottom: 20px;
	          li + li {
	            margin-left: 5px;
	          }
	        `};
	`;
	
	const sharedTitleSubTitle = props => {
	  switch (props.status) {
	    case 'success':
	      return css`
	        color: ${props.theme.color['success-dark']};
	        ${Icon} {
	          color: ${props.theme.color.success};
	        }
	      `;
	    case 'error':
	      return css`
	        color: ${props.theme.color.error};
	      `;
	    case 'warning':
	      return css`
	        color: ${props.theme.color['warning-dark']};
	        ${Icon} {
	          color: ${props.theme.color.warning};
	        }
	      `;
	    case 'loading':
	      return css`
	        color: ${props.theme.color.base};
	      `;
	  }
	};
	
	const cursorStyles = props => {
	  if (props.disabled) {
	    return 'not-allowed';
	  }
	  if (!props.isActive) {
	    return 'pointer';
	  }
	  return null;
	};
	
	export const Title = styled.button`
	  background-color: ${({theme, isActive, disabled}) => {
	    if (disabled) {
	      return theme.color.blackTint4;
	    }
	    if (isActive) {
	      return theme.color.white;
	    }
	    return theme.color.blackTint4;
	  }};
	  border: solid 1px ${({theme}) => theme.color.blackTint3};
	  color: ${({theme, isActive, disabled}) =>
	    isActive || disabled ? theme.color.base : theme.color.primary};
	  font: inherit;
	  font-weight: bold;
	  padding: 10px 15px;
	  position: relative;
	  transition-property: background-color, color;
	  transition-duration: 0.2s;
	  transition-timing-function: ease-in;
	  cursor: ${cursorStyles};
	  opacity: ${props => props.disabled && '0.5'};
	
	  &:focus {
	    outline: ${({disabled, theme}) =>
	      disabled ? 0 : `1px dashed ${theme.color.blackTint3}`};
	    outline-offset: 2px;
	  }
	
	  &:hover,
	  &:focus {
	    background-color: ${({isActive, disabled, theme}) =>
	      !isActive && !disabled && theme.color.white};
	  }
	
	  ${props =>
	    props.isVertical
	      ? css`
	          // vertical tabs need a fixed width, while the horizontal above is flexible
	          width: 200px;
	          // so the tab's border properly overlays the whole border
	          left: 1px;
	          &.is-active {
	            border-right: 1px solid ${props.theme.color.white};
	          }
	          ${Icon} {
	            position: absolute;
	            right: 15px;
	          }
	        `
	      : css`
	          // so the tab's border properly overlays the whole border
	          top: 1px;
	          height: 100%;
	          &.is-active {
	            border-bottom: 1px solid ${props.theme.color.white};
	          }
	          & + & {
	            margin-left: 5px;
	          }
	          ${Icon} {
	            position: absolute;
	            left: 15px;
	          }
	        `}
	
	  ${sharedTitleSubTitle};
	`;
	
	export const TitleContent = styled.span`
	  display: inline-block;
	  vertical-align: baseline;
	  height: 100%;
	`;
	
	export const Icon = styled.span`
	  margin-right: ${props => props.theme.spacing.small};
	  vertical-align: text-top;
	`;
	
	export const SubTitle = styled.button`
	  font: inherit;
	  color: ${({theme, isActive}) =>
	    isActive ? theme.color.base : theme.color.primary};
	  cursor: ${cursorStyles};
	  // Avoid vertical shifting from bold
	  line-height: 1;
	  text-decoration: 
	opacity: ${({disabled}) => disabled && '0.5'};
	
	  ${TitleContent} {
	    text-decoration: ${({isActive, disabled}) =>
	      isActive || disabled ? 'none' : 'underline'};;
	  }
	
	  &:first-of-type {
	    padding-left: 0;
	  }
	  &:last-of-type {
	    padding-right: 0;
	    border-right: 0;
	  }
	
	  &:hover {
	    color: ${({status, theme, isActive, disabled}) => {
	      if (!isActive && !disabled) {
	        switch (status) {
	          case 'success':
	            return lighten(0.1, theme.color.success);
	          case 'error':
	            return lighten(0.1, theme.color.error);
	          case 'warning':
	            return lighten(0.1, theme.color.warning);
	          case 'loading':
	            return lighten(0.1, theme.color.base);
	        }
	      }
	    }}
	    ${Icon} {
	      color: ${({status, theme, isActive, disabled}) => {
	        if (!isActive && !disabled) {
	          switch (status) {
	            case 'success':
	              return lighten(0.1, theme.color['success-dark']);
	            case 'warning':
	              return lighten(0.1, theme.color['warning-dark']);
	          }
	        }
	      }}
	    }
	  }
	
	  &:focus {
	    outline: ${({disabled, theme}) =>
	      !disabled && `1px dashed ${theme.color.blackTint3}`}
	    outline-offset: 2px;
	  }
	
	${sharedTitleSubTitle};
	`;
	
	export const Content = styled.div`
	  display: ${({isActive}) => !isActive && 'none'};
	  width: 100%;
	  &:focus {
	    outline: 0;
	  }
	`;
