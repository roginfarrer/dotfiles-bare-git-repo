5424d60733d2d912b1031aa015cec909|file:///Users/rfarrer/Wayfair/homebase/packages/homebase-cli/src/utils/exec.js|10|1|V|javascript
	const {spawn} = cp;
	
	// async version of child_process.exec
	export default function exec(command, args, config = {}) {
	  return new Promise((resolve, reject) => {
	    const proc = spawn(command, args, {
	      // prevents a new terminal from popping up on windows
	      windowsHide: true,
	      shell: false,
	      stdio: ['ignore', 1, 2],
	      ...config,
	    });
	    proc.on('error', ex => {
	      reject(ex);
	    });
	
	    proc.on('close', code => {
	      if (code !== 0) {
	        reject(new Error('Unknown shell error'));
	      }
	      resolve();
	    });
	  });
	}
