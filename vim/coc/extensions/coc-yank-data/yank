cce4be9f95bc4e53a52027a1dc1311a5|file:///Users/rfarrer/Wayfair/homebase/develop-sync.js|7|3|V|javascript
	/* eslint-disable no-console */
	import cp from 'child_process';
	const {spawn} = cp;
	
	// async version of child_process.exec
	function exec(command, args, config = {}) {
	  return new Promise((resolve, reject) => {
	    const proc = spawn(command, args, {
	      // prevents a new terminal from popping up on windows
	      windowsHide: true,
	      shell: false,
	      stdio: ['ignore', 1, 2],
	      ...config,
	    });
	    proc.on('error', ex => {
	      reject(ex);
	    });
	
	    proc.on('close', code => {
	      if (code !== 0) {
	        reject(new Error('Unknown shell error'));
	      }
	      resolve();
	    });
	  });
	}
	
