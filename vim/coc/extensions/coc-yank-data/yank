	import BaseButton from '../button-base/base-button';
	import Icon from '../icon/icon';
	import {faQuestionCircle} from '@fortawesome/free-solid-svg-icons/faQuestionCircle';
	import {generateTestIds, extendTestIds} from '@wayfair/homebase-common';
	import Lnrs from '@wayfair/lnrs';
a9eb4705d80eee3edfe5267216255880|file:///Users/rfarrer/Wayfair/homebase/packages/extranet-components/src/page-container/page-container.js|18|1|V|javascript
	// Context is used to manage semantic markup for this component and
	// components used within it. For example, the UtilityBar uses this
	// context to avoid using a <header> element, since this component
	// already provides one.
	const PageContainerContext = React.createContext(false);
	
	const PageContainer = ({
	  breadcrumbs,
	  pageTitle,
	  children,
	  utilityBar,
	  showBackground,
	  layoutWidth,
	  pageDescription,
	  actions,
	  testIds,
	}) => {
	  const containerClasses = cx('ex-PageContainer', {
	    'ex-PageContainer--fullWidth': layoutWidth === 'fullWidth',
	    'ex-PageContainer--responsive': layoutWidth === 'responsive',
	  });
	  const mainClasses = cx('ex-PageContainer-content', {
	    'ex-PageContainer-content--background': showBackground,
	  });
	
	  return (
	    <div {...generateTestIds({testIds})} className={containerClasses}>
	      <header className="ex-PageContainer-header">
	        <PageContainerContext.Provider value>
	          {breadcrumbs && (
	            <div className="ex-PageContainer-breadcrumbs">
	              <Breadcrumbs
	                testIds={extendTestIds({testIds, postfix: 'breadcrumbs'})}
	                // We need the spacing for the UtilityBar, but if just the PageTitle is rendered,
	                // its line-height creates enough vertical spacing
	                hasNoVerticalSpacing={!utilityBar}
	                breadcrumbs={breadcrumbs}
	              />
	            </div>
	          )}
	          <div className="ex-PageContainer-titlebar">
	            <div
	              {...generateTestIds({testIds, postfix: 'pageTitle'})}
	              className="ex-PageContainer-title"
	            >
	              {pageTitle && <PageTitle mb={null}>{pageTitle}</PageTitle>}
	              {utilityBar}
	            </div>
	            {actions && (
	              <div className="ex-PageContainer-actions">{actions}</div>
	            )}
	          </div>
	          {pageDescription && (
	            <p
	              {...generateTestIds({testIds, postfix: 'pageDescription'})}
	              className="ex-PageContainer-description"
	            >
	              {pageDescription}
	            </p>
	          )}
	        </PageContainerContext.Provider>
	      </header>
	      <main
	        {...generateTestIds({testIds, postfix: 'main'})}
	        className={mainClasses}
	      >
	        {children}
	      </main>
	    </div>
	  );
	};
	
	const HelpLink = props => {
	  const context = React.useContext(PageContainerContext);
	
	  // Make sure this component is only used in the header of the PageContainer
	  // While it could be made flexible to use elsewhere, that's not its intended
	  // use, and its implementation elsewhere could make it harder to update
	  if (!context) {
	    throw new Error(
	      'PageContainer.HelpLink must be rendered within the header of a PageContainer component.'
	    );
	  }
	
	  return (
	    <BaseButton {...props} className="ex-PageContainer-helpLink">
	      <span className="ex-PageContainer-helpLink-icon">
	        <Icon icon={faQuestionCircle} />
	      </span>
	      <Lnrs key="Help">Help</Lnrs>
	    </BaseButton>
	  );
	};
	
	PageContainer.HelpLink = HelpLink;
	PageContainer.Context = PageContainerContext;
	
	PageContainer.propTypes = {
	  // Set the breadcrumbs of the page
	  breadcrumbs: PropTypes.array,
	  // Set the page's title. Only meant to support Lnrs components.
	  pageTitle: PropTypes.node,
	  // Set the layout width of the page
	  layoutWidth: PropTypes.oneOf(['default', 'fullWidth', 'responsive'])
	    .isRequired,
	  // If true, page content renders within a white background
	  showBackground: PropTypes.bool,
	  // The content of the page
	  children: PropTypes.node,
	  // Pass in a `<UtilityBar>` component from `extranet_utility_bar` instead of page title or breadcrumbs
	  utilityBar: PropTypes.node,
	  // Renders a brief, informative description of the page below the title
	  pageDescription: PropTypes.node,
	  // Contents of the action bar to be rendered next to the page title
	  actions: PropTypes.node,
	  // Optional Enzyme and Codeception testing identifiers
	  testIds: PropTypes.shape({
	    enzyme: PropTypes.string,
	    codeception: PropTypes.string,
	  }),
	};
	PageContainer.defaultProps = {
	  breadcrumbs: null,
	  pageTitle: null,
	  layoutWidth: 'default',
	  showBackground: true,
	  children: null,
	  utilityBar: null,
	  pageDescription: null,
	  actions: null,
	  testIds: {},
	};
	
	export default PageContainer;
a446a2301c08d87c273927a6f1f111e2|file:///Users/rfarrer/Wayfair/homebase/packages/admin-components/src/page-container/page-container.js|18|1|V|javascript
	const Page = ({breadcrumbs, pageTitle, children, utilityBar, layoutWidth}) => {
	  const containerClasses = cx('admin-PageContainer', {
	    'admin-PageContainer--fullWidth': layoutWidth === 'fullWidth',
	    'admin-PageContainer--responsive': layoutWidth === 'responsive',
	  });
	
	  return (
	    <div className={containerClasses}>
	      <header>
	        {breadcrumbs && <Breadcrumbs breadcrumbs={breadcrumbs} />}
	        {pageTitle && !utilityBar && <PageTitle>{pageTitle}</PageTitle>}
	        {utilityBar}
	      </header>
	      <main>{children}</main>
	    </div>
	  );
	};
	
	Page.propTypes = {
	  // Set the breadcrumbs of the page
	  breadcrumbs: PropTypes.array,
	  // Set the page's title. Only meant to support Lnrs components.
	  pageTitle: PropTypes.node,
	  // Set the layout width of the page
	  layoutWidth: PropTypes.oneOf(['default', 'fullWidth', 'responsive']),
	  // The content of the page
	  children: PropTypes.node,
	  // Pass in a `<UtilityBar>` component from `admin_utility_bar` instead of page title or breadcrumbs
	  utilityBar: PropTypes.node,
	};
	
	Page.defaultProps = {
	  breadcrumbs: null,
	  pageTitle: null,
	  layoutWidth: 'default',
	  children: null,
	  utilityBar: null,
	};
	
	export default Page;
