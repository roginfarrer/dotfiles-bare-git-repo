cfffcdcc4b7ca6dd33b6a5777ab26cf5|file:///Users/rfarrer/Wayfair/homebase/packages/admin-components/src/selectable/selectable.js|104|1|V|javascript
	SelectableInput.propTypes = {
	  // Presentational component that receives context
	  children: PropTypes.oneOfType([PropTypes.node, PropTypes.func]).isRequired,
	  // If true, the component has been checked
	  // eslint-disable-next-line react/require-default-props
	  checked: PropTypes.bool,
	  // If true, the component defaults to checked and is an uncontrolled input.
	  defaultChecked: PropTypes.bool,
	  // If true, the component is disabled
	  disabled: PropTypes.bool,
	  // Function called when the component is checked
	  onChange: PropTypes.func,
	  // Function called when the component is checked
	  onFocus: PropTypes.func,
	  // Function called when the component is checked
	  onBlur: PropTypes.func,
	  // The type of input
	  variation: PropTypes.oneOf(['checkbox', 'radio']),
	  // The name attribute of the input
	  name: PropTypes.string,
	  // The value attribute of the input. Required when used as a radio
	  value: PropTypes.oneOfType([PropTypes.string, PropTypes.number]),
	};
	
	SelectableInput.defaultProps = {
	  defaultChecked: false,
	  disabled: false,
	  onChange() {},
	  onFocus() {},
	  onBlur() {},
	  variation: 'checkbox',
	  name: null,
	};
