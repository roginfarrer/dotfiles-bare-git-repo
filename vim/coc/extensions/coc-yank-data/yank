	### `0.0.110` (October 25, 2019)
38e19e926170201a70ff57ad53283307|file:///Users/rfarrer/Wayfair/monolith/resources/st4/includes/js/amd_modules/admin/react/_components/table/docs/admin-table-editable-new-demo.js|1|1|V|javascript
	import React from 'react';
	import Table, {CELL_ALIGNMENT} from 'admin_table';
	import TableInput from 'admin_table_input';
	import TableDropdown from 'admin_table_dropdown';
	import TableSelect from 'admin_table_select';
	import TableTextarea from 'admin_table_textarea';
	import TableDatepicker from 'admin_table_datepicker';
	import ValidationTooltip from 'admin_input_validation_tooltip';
	import {
	  data as tableData,
	  selectOptions,
	  invalidSelectOptions,
	  dropdownOptions,
	  invalidDropdownOptions
	} from 'homebase_demo_data';
	import dateFns from 'date_fns';
	
	// Data used here is the following shape, but the objects can be any shape at all
	// [
	//   {...},
	//   {
	//     first_name: 'Soledad',
	//     last_name: 'Mockus',
	//     company_name: 'Sinclair Machine Products Inc',
	//     address: '75 Elm Rd #1190',
	//     state: 'ACT',
	//     post: 2600,
	//     city: 'Barton',
	//     phone1: '02-1291-8182',
	//     phone2: '0444-126-746',
	//     email: 'soledad_mockus@yahoo.com',
	//     web: 'http://www.sinclairmachineproductsinc.com.au'
	//   },
	//   {...}
	// ]
	
	class EditableTable extends React.Component {
	  state = {
	    data: tableData,
	    inputValidity: [],
	    selectValidity: [],
	    textareaValidity: [],
	    dropdownValidity: []
	  };
	
	  columns = [
	    {
	      Header: 'Airline',
	      accessor: 'title',
	      editable: true,
	      Cell: row => {
	        return (
	          <React.Fragment>
	            <TableSelect
	              onChange={e => this.handleSelectChange(e, row)}
	              defaultValue={this.state.data[row.index][row.column.id]}
	              validity={
	                this.state.selectValidity.includes(row.index) ? 'invalid' : null
	              }
	              options={selectOptions}
	            />
	            {this.state.selectValidity.includes(row.index) && (
	              <ValidationTooltip openOnValidation relative>
	                This airline is invalid.
	              </ValidationTooltip>
	            )}
	          </React.Fragment>
	        );
	      }
	    },
	    {
	      Header: 'Aircraft',
	      accessor: 'aircraft',
	      editable: true,
	      Cell: row => {
	        return (
	          <React.Fragment>
	            <TableDropdown
	              label={row.column.id}
	              onValueChange={value => this.handleDropdownChange(value, row)}
	              value={dropdownOptions.find(
	                opt => opt.value === this.state.data[row.index][row.column.id]
	              )}
	              validity={
	                this.state.dropdownValidity.includes(row.index)
	                  ? 'invalid'
	                  : null
	              }
	              options={dropdownOptions}
	              inputProps={{name: 'dropdown'}}
	            />
	            {this.state.dropdownValidity.includes(row.index) && (
	              <ValidationTooltip openOnValidation relative>
	                This airline is invalid.
	              </ValidationTooltip>
	            )}
	          </React.Fragment>
	        );
	      }
	    },
	    {
	      Header: 'Author',
	      accessor: 'author',
	      editable: true,
	      Cell: row => (
	        <React.Fragment>
	          <TableInput
	            onSubmit={e => this.handleInputSubmit(e, {row, name: 'input'})}
	            defaultValue={this.state.data[row.index][row.column.id]}
	            validity={
	              this.state.inputValidity.includes(row.index) ? 'invalid' : null
	            }
	            inputProps={{
	              name: 'input'
	            }}
	          />
	          {this.state.inputValidity.includes(row.index) && (
	            <ValidationTooltip openOnValidation relative>
	              Input must be longer than 5 characters.
	            </ValidationTooltip>
	          )}
	        </React.Fragment>
	      )
	    },
	    {
	      Header: 'Date',
	      accessor: 'date',
	      editable: true,
	      Cell: row => (
	        <TableDatepicker
	          value={dateFns.format(
	            this.state.data[row.index][row.column.id],
	            'MMMM DD, YYYY'
	          )}
	          onDayChange={day => this.handleDayChange(day, row)}
	          dayPickerProps={{
	            selectedDays: this.state.selectedDay
	          }}
	        />
	      ),
	      sortMethod(a, b) {
	        return new Date(a) - new Date(b);
	      }
	    },
	    {
	      Header: 'Review',
	      accessor: 'content',
	      editable: true,
	      Cell: row => (
	        <React.Fragment>
	          <TableTextarea
	            onSubmit={e => this.handleInputSubmit(e, {row, name: 'textarea'})}
	            defaultValue={this.state.data[row.index][row.column.id]}
	            inputProps={{
	              name: 'textarea'
	            }}
	            validity={
	              this.state.textareaValidity.includes(row.index) ? 'invalid' : null
	            }
	          />
	          {this.state.textareaValidity.includes(row.index) && (
	            <ValidationTooltip openOnValidation relative>
	              Input must be longer than 20 characters
	            </ValidationTooltip>
	          )}
	        </React.Fragment>
	      )
	    },
	    {
	      Header: 'Rating',
	      accessor: 'overall_rating',
	      maxWidth: 75
	    }
	  ];
	
	  handleInputSubmit = (e, {row, name}) => {
	    const {type} = e;
	    let value;
	    if (type === 'blur') {
	      // if its a blur event, then the event is from the input
	      ({value} = e.target);
	    } else {
	      // otherwise we assume the event is from a form submission
	      ({value} = e.target[name]);
	    }
	    this.setState(prevState => {
	      const inputLength = name === 'input' ? 5 : 20;
	      const validityStateInput =
	        name === 'input' ? 'inputValidity' : 'textareaValidity';
	
	      // copy old state
	      let newValidityState = [...prevState[validityStateInput]];
	      if (
	        value.length < inputLength &&
	        !prevState[validityStateInput].includes(row.index)
	      ) {
	        // If invalid, add this field to invalid inputs
	        newValidityState = [...prevState[validityStateInput], row.index];
	        // If is valid, and this field was invalid
	      } else if (
	        value.length > inputLength &&
	        prevState[validityStateInput].includes(row.index)
	      ) {
	        // Find the index of this invalid field in state
	        const index = prevState[validityStateInput].indexOf(row.index);
	        // And filter it out of state
	        newValidityState = [...prevState[validityStateInput]].filter(
	          (_, i) => i !== index
	        );
	      }
	
	      return {
	        [validityStateInput]: newValidityState,
	        data: [
	          ...prevState.data.slice(0, row.index),
	          {
	            ...prevState.data[row.index],
	            [row.column.id]: value
	          },
	          ...prevState.data.slice(row.index + 1)
	        ]
	      };
	    });
	  };
	
	  handleSelectChange = (e, row) => {
	    const {value} = e.target;
	    this.setState(prevState => {
	      // copy old state
	      let newValidityState = [...prevState.selectValidity];
	      if (
	        invalidSelectOptions.includes(value) &&
	        !prevState.selectValidity.includes(row.index)
	      ) {
	        // If invalid, add this field to invalid inputs
	        newValidityState = [...prevState.selectValidity, row.index];
	        // If is valid, and this field was invalid
	      } else if (
	        !invalidSelectOptions.includes(value) &&
	        prevState.selectValidity.includes(row.index)
	      ) {
	        // Find the index of this invalid field in state
	        const index = prevState.selectValidity.indexOf(row.index);
	        // And filter it out of state
	        newValidityState = [...prevState.selectValidity].filter(
	          (_, i) => i !== index
	        );
	      }
	      return {
	        selectValidity: newValidityState,
	        data: [
	          ...prevState.data.slice(0, row.index),
	          {
	            ...prevState.data[row.index],
	            [row.column.id]: value
	          },
	          ...prevState.data.slice(row.index + 1)
	        ]
	      };
	    });
	  };
	
	  handleDropdownChange = (value, row) => {
	    this.setState(prevState => {
	      const newValue = value ? value.value : '';
	      // copy old state
	      let newValidityState = [...prevState.dropdownValidity];
	      if (
	        invalidDropdownOptions.includes(newValue) &&
	        !prevState.dropdownValidity.includes(row.index)
	      ) {
	        // If invalid, add this field to invalid inputs
	        newValidityState = [...prevState.dropdownValidity, row.index];
	        // If is valid, and this field was invalid
	      } else if (
	        !invalidDropdownOptions.includes(newValue) &&
	        prevState.dropdownValidity.includes(row.index)
	      ) {
	        // Find the index of this invalid field in state
	        const index = prevState.dropdownValidity.indexOf(row.index);
	        // And filter it out of state
	        newValidityState = [...prevState.dropdownValidity].filter(
	          (_, i) => i !== index
	        );
	      }
	      return {
	        dropdownValidity: newValidityState,
	        data: [
	          ...prevState.data.slice(0, row.index),
	          {
	            ...prevState.data[row.index],
	            [row.column.id]: newValue
	          },
	          ...prevState.data.slice(row.index + 1)
	        ]
	      };
	    });
	  };
	
	  handleDayChange = (day, row) => {
	    this.setState(prevState => {
	      return {
	        data: [
	          ...prevState.data.slice(0, row.index),
	          {
	            ...prevState.data[row.index],
	            [row.column.id]: day
	          },
	          ...prevState.data.slice(row.index + 1)
	        ]
	      };
	    });
	  };
	
	  render() {
	    return (
	      <Table
	        cellAlignment={CELL_ALIGNMENT.TOP_LEFT}
	        striped
	        data={this.state.data}
	        columns={this.columns}
	        showOverflow
	        defaultPageSize={5}
	        useNewInputStyle
	      />
	    );
	  }
	}
	
	export default EditableTable;
573fa9d420b5f7cf963a908b61f7438c|file:///Users/rfarrer/Wayfair/monolith/resources/st4/includes/js/amd_modules/admin/react/_components/table/docs/admin-table-editable-updated-demo.js|51|20|v|javascript
	{
	        return (
	          <React.Fragment>
	            <TableSelect
	              onChange={e => this.handleSelectChange(e, row)}
	              defaultValue={this.state.data[row.index][row.column.id]}
	              validity={
	                this.state.selectValidity.includes(row.index) ? 'invalid' : null
	              }
	              options={selectOptions}
	            />
