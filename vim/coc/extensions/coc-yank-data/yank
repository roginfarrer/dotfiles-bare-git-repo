	import React from 'react';
	import {Selectable} from 'extranet-selectable';
	import Image from 'extranet_image';
	import VisuallyHidden from 'extranet-visually-hidden';
	
	const Demo = () => {
	  const [checked, setChecked] = React.useState(false);
	
	  return (
	    <Selectable
	      checked={checked}
	      onChange={() => setChecked(oldChecked => !oldChecked)}
	    >
	      <Image alt="" imageId={81751731} />
	      <VisuallyHidden>Label text for accessibility</VisuallyHidden>
	    </Selectable>
	  );
	};
	
	export default Demo;
