	  static propTypes = {
	    // Text for the heading of the drawer
	    heading: PropTypes.string,
	    // An element or group of elements for the top of the drawer. If provided, will override the `heading` prop.
	    header: PropTypes.node,
	    // Content of the drawer
	    children: PropTypes.node,
	    // Which side of the window the drawer originates from
	    drawerSide: PropTypes.oneOf(['left', 'right']),
	    // If true, the drawer is rendered open
	    isOpen: PropTypes.bool.isRequired,
	    // If true, close button is rendered in the heading
	    showClose: PropTypes.bool,
	    // If true, adds styling and functionality that allows the use of the docked DrawerTabs
	    useTabs: PropTypes.bool,
	    // Handler called when drawer is closed
	    onRequestClose: PropTypes.func,
	    // If true, clicking on the overlay allows the drawer to close
	    clickOverlayToClose: PropTypes.bool,
	    // Affixes an element to the bottom of the drawer. Should probably be a button.
	    ctaElement: deprecatedProp(
	      'The `ctaElement` prop is deprecated. Use the `footer` prop instead'
	    ),
	    // Affixes an element or group of elements to the bottom of the drawer
	    footer: PropTypes.node,
	    // The active tab. This is needed if you are using DrawerTabs.
	    activeTabId: isValidTabId,
	    // If true, an overlay or veil will display below the drawer
	    hasOverlay: PropTypes.bool,
	    // If true, back button is rendered in the heading
	    showBack: PropTypes.bool,
	    // Handler called when the back button is clicked
	    onBackClick: PropTypes.func,
	    // Set the width of the drawer (defaults to 400px)
	    width: PropTypes.number,
	    // The color of the background of the overlay
	    overlayColor: PropTypes.oneOf(['dark', 'light']),
	    // The background of the content area; defaults to 'white'
	    contentBackgroundColor: PropTypes.oneOf(['white', 'lightGrey']),
	  };
	
	  static defaultProps = {
	    heading: '',
	    header: null,
	    children: null,
	    drawerSide: 'right',
	    showClose: true,
	    useTabs: false,
	    onRequestClose() {},
	    clickOverlayToClose: true,
	    ctaElement: null,
	    footer: null,
	    activeTabId: '',
	    hasOverlay: false,
	    showBack: false,
	    onBackClick: null,
	    width: null,
	    overlayColor: Drawer.OVERLAY_COLORS.DARK,
	    contentBackgroundColor: 'white',
	  };
