0612829e4bcd9b2073badb57dd8b3df6|file:///Users/rfarrer/Wayfair/homebase/progress-bar.test.js|13|1|V|javascript
	test('each component instance has a unique id, and fill mask references the correct ID', () => {
	  const {getByTestId} = render(
	    <>
	      <ProgressBar testIds={{enzyme: 'foo'}} current={75} />
	      <ProgressBar testIds={{enzyme: 'bar'}} current={25} />
	      <ProgressBar testIds={{enzyme: 'baz'}} current={50} />
	    </>
	  );
	
	  // First Progress Bar
	  expect(getByTestId('foo-mask')).toHaveAttribute(
	    'id',
	    expect.stringContaining('0')
	  );
	  expect(getByTestId('foo-fill-mask')).toHaveAttribute(
	    'mask',
	    expect.stringContaining('0')
	  );
	  // The mask reference should contain the mask's ID
	  expect(getByTestId('foo-fill-mask').getAttribute('mask')).toContain(
	    getByTestId('foo-mask').id
	  );
	
	  // Second Progress Bar
	  expect(getByTestId('bar-mask')).toHaveAttribute(
	    'id',
	    expect.stringContaining('1')
	  );
	  expect(getByTestId('bar-fill-mask')).toHaveAttribute(
	    'mask',
	    expect.stringContaining('1')
	  );
	  // The mask reference should contain the mask's ID
	  expect(getByTestId('bar-fill-mask').getAttribute('mask')).toContain(
	    getByTestId('bar-mask').id
	  );
	
	  // Third Progress Bar
	  expect(getByTestId('baz-mask')).toHaveAttribute(
	    'id',
	    expect.stringContaining('2')
	  );
	  expect(getByTestId('baz-fill-mask')).toHaveAttribute(
	    'mask',
	    expect.stringContaining('2')
	  );
	  // The mask reference should contain the mask's ID
	  expect(getByTestId('baz-fill-mask').getAttribute('mask')).toContain(
	    getByTestId('baz-mask').id
	  );
	});
