08692e07affcd54cbd0a4a4f6af9bc87|file:///Users/rfarrer/Wayfair/homebase/packages/admin-components/src/toast/toast.scss|1|1|V|scss
	////
	/// Admin Toast Component
	///
	/// @author    Ryan Herlihy <rherlihy@wayfair.com>
	/// @copyright 2018 Wayfair LLC - All rights reserved
	////
	
	@function tint($color) {
	  @return mix($bg-color--white, $color, 10%);
	}
	
	.admin-Toast {
	  $root: &;
	  @include componentScope($color: $text-color--white);
	
	  margin: 0 auto;
	  width: 390px;
	  background-color: $bg-color--dark;
	  border: 1px solid transparent;
	  border-radius: 5px;
	  position: fixed;
	  left: 50%;
	  overflow-wrap: break-word;
	  display: flex;
	  a {
	    &,
	    &:hover,
	    &:focus {
	      color: $text-color--white;
	    }
	  }
	  z-index: $z--3000; // below modal wrap
	  &-close {
	    z-index: 1; // above toast-message
	    align-items: center;
	    display: flex;
	    margin-right: $spacing--2;
	  }
	  &-message {
	    position: relative;
	    padding: $spacing--2;
	    margin: 0;
	    min-height: 60px;
	    display: flex;
	    align-items: center;
	    flex-grow: 1;
	    justify-content: center;
	    font-weight: bold;
	    .is-dismissible &,
	    .has-status & {
	      justify-content: flex-start;
	    }
	  }
	  &-statusIcon {
	    align-items: center;
	    display: flex;
	    padding: 0 $spacing--2;
	    font-size: 22px;
	    color: $text-color--white;
	    &--error {
	      background-color: $bg-color--error;
	    }
	    &--success {
	      background-color: $bg-color--success;
	    }
	    &--warning {
	      background-color: $bg-color--warning;
	    }
	  }
	  &-transition {
	    &--fadeInBottom {
	      animation-duration: 1s;
	      animation-name: toastFadeInBottom;
	      animation-fill-mode: both;
	    }
	  }
	  &--overModal {
	    z-index: $z--6000; // Override z-index so the toast shows up over modals and modal overlays
	  }
	  &--error {
	    background-color: tint($bg-color--error);
	    border-color: $border-color--error;
	    color: $text-color--error;
	  }
	  &--success {
	    background-color: tint($bg-color--success);
	    border-color: $border-color--success;
	    color: $text-color--success;
	  }
	  &--warning {
	    background-color: tint($bg-color--warning);
	    border-color: $border-color--warning;
	    color: $text-color--warning;
	  }
	  &:focus {
	    outline: 0;
	  }
	}
	
	@keyframes toastFadeInBottom {
	  0% {
	    opacity: 0;
	    transform: translateX(-50%) translateY(100%);
	  }
	  75% {
	    transform: translateX(-50%) translateY(0);
	  }
	  100% {
	    opacity: 1;
	    transform: translateX(-50%);
	  }
	}
