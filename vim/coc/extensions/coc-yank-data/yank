	        {({exists}) => {
	          const Element = exists ? 'div' : 'header';
	          return (
	            <Element className="ex-UtilityBar" {...generateTestIds({testIds})}>
	              <nav className="ex-UtilityBar-nav js-utilitybar-nav">
	                <h1
	                  {...generateTestIds({testIds, postfix: 'title'})}
	                  className="ex-UtilityBar-title"
	                  id="utilitybar-title"
	                >
	                  {pageTitle}
	                </h1>
	                <ul
	                  {...generateTestIds({testIds, postfix: 'nav'})}
	                  className={cx('ex-UtilityBar-nav-list', {
	                    'is-hidden': shouldHideVisibleList,
	                  })}
	                  // We must render this element since it's needed to calculate the presence of visible items
	                  // However, we can hide it if no visible items are rendered
	                  aria-hidden={shouldHideVisibleList ? 'true' : null}
	                  ref={ul => (this.navList = ul)}
	                  role="menubar"
	                  aria-labelledby="utilitybar-title"
	                >
	                  {visibleItems}
	                </ul>
	                {this.showButton() && (
	                  <button
	                    className="ex-UtilityBar-nav-btn"
	                    onClick={this.toggleHiddenNav}
	                    aria-haspopup="true"
	                  >
	                    <VisuallyHidden>
	                      <Lnrs key="ShowMore">Show more</Lnrs>
	                    </VisuallyHidden>
	                    <span className={navButtonIconClasses} aria-hidden="true" />
	                  </button>
	                )}
	                {Array.isArray(hiddenItems) && hiddenItems.length > 0 && (
	                  <ul
	                    className={hiddenListClasses}
	                    role="menubar"
	                    aria-labelledby="utilitybar-title"
	                  >
	                    {hiddenItems}
	                  </ul>
	                )}
	              </nav>
	            </Element>
	          );
	        }}
	      
