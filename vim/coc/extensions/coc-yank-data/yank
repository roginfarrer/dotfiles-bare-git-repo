02b22d2251f131c720716a5111091b01|file:///Users/rfarrer/Wayfair/homebase/packages/homebase-cli/src/monolith.js|42|1|V|javascript
	// async version of child_process.exec
	function exec(command, args, config = {}) {
	  return new Promise((resolve, reject) => {
	    const proc = spawn(command, args, {
	      // prevents a new terminal from popping up on windows
	      windowsHide: true,
	      shell: false,
	      stdio: ['ignore', 1, 2],
	      ...config,
	    });
	    proc.on('error', ex => {
	      reject(ex);
	    });
	
	    proc.on('close', code => {
	      if (code !== 0) {
	        reject(new Error('Unknown shell error'));
	      }
	      resolve();
	    });
	  });
	}
